<h1>Please fill required information to make a new post</h1>
<%= form_with model:@post, url:user_posts_path do |f| %>
  <div class="flex flex-col">
    <%= f.label :title, "title" %>
    <%= f.text_field :title %>
    <%= f.label :text, "text" %>
    <%= f.text_area :text %>
    <%= f.submit "Submit"%>
  </div>
<% end %>
